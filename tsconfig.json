{
	"compilerOptions": {
		"target": "ES2021",
		"strict": true,
		"noImplicitAny": true,
		"noImplicitOverride": true,
		"noUncheckedIndexedAccess": true,
		"noPropertyAccessFromIndexSignature": true,
		"skipLibCheck": true,
		"allowJs": true,
		"noEmit": true,
		"moduleResolution": "nodenext",
		"module": "NodeNext"
	},
	"references": [{ "path": "./tsconfig.vite.json" }]
}
